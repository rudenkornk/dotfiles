// Qwerty for programmers
partial alphanumeric_keys
xkb_symbols "qwerty_for_programmers" {

    include "us"
    name[Group1]= "English (Qwerty for programmers)";

    key <TLDE> { [       ssharp,        U0122,      asciitilde,       asciitilde ] };
    key <AE01> { [            1,       exclam,          exclam,       asciitilde ] };
    key <AE02> { [            2,           at,              at,       asciitilde ] };
    key <AE03> { [            3,   numbersign,      numbersign,       asciitilde ] };
    key <AE04> { [            4,       dollar,          dollar,       asciitilde ] };
    key <AE05> { [            5,      percent,         percent,       asciitilde ] };
    key <AE06> { [            6,  asciicircum,     asciicircum,       asciitilde ] };
    key <AE07> { [            7,    ampersand,       ampersand,       asciitilde ] };
    key <AE08> { [            8,     asterisk,        asterisk,       asciitilde ] };
    key <AE09> { [            9,    parenleft,       parenleft,       asciitilde ] };
    key <AE10> { [            0,   parenright,      parenright,       asciitilde ] };
    key <AE11> { [        minus,   underscore,      underscore,       asciitilde ] };
    key <AE12> { [        equal,         plus,            plus,       asciitilde ] };

    key <AD01> { [            q,            Q,         percent,       asciitilde ] };
    key <AD02> { [            w,            W,     asciicircum,       asciitilde ] };
    key <AD03> { [            e,            E,           minus,       asciitilde ] };
    key <AD04> { [            r,            R,            plus,       asciitilde ] };
    key <AD05> { [            t,            T,        asterisk,       asciitilde ] };
    key <AD06> { [            y,            Y,             bar,       asciitilde ] };
    key <AD07> { [            u,            U,       ampersand,       asciitilde ] };
    key <AD08> { [            i,            I,       braceleft,                8 ] };
    key <AD09> { [            o,            O,      braceright,                9 ] };
    key <AD10> { [            p,            P,       semicolon,       asciitilde ] };
    key <AD11> { [   udiaeresis,   Udiaeresis,      apostrophe,       asciitilde ] };
    key <AD12> { [   ydiaeresis,   Ydiaeresis,           grave,       asciitilde ] };
    key <BKSL> { [    backslash,          bar,             bar,       asciitilde ] };

    key <AC01> { [            a,            A,          exclam,       asciitilde ] };
    key <AC02> { [            s,            S,      asciitilde,       asciitilde ] };
    key <AC03> { [            d,            D,            less,       asciitilde ] };
    key <AC04> { [            f,            F,           equal,       asciitilde ] };
    key <AC05> { [            g,            G,         greater,       asciitilde ] };
    key <AC06> { [            h,            H,           slash,                0 ] };
    key <AC07> { [            j,            J,       parenleft,                1 ] };
    key <AC08> { [            k,            K,      parenright,                2 ] };
    key <AC09> { [            l,            L,      underscore,                3 ] };
    key <AC10> { [   odiaeresis,   Odiaeresis,           colon,                4 ] };
    key <AC11> { [   adiaeresis,   Adiaeresis,        quotedbl,       asciitilde ] };

    key <AB01> { [            z,            Z,      asciitilde,       asciitilde ] };
    key <AB02> { [            x,            X,      asciitilde,       asciitilde ] };
    key <AB03> { [            c,            C,      asciitilde,       asciitilde ] };
    key <AB04> { [            v,            V,          dollar,       asciitilde ] };
    key <AB05> { [            b,            B,       backslash,                5 ] };
    key <AB06> { [            n,            N,     bracketleft,                6 ] };
    key <AB07> { [            m,            M,    bracketright,                7 ] };
    key <AB08> { [   idiaeresis,   Idiaeresis,      asciitilde,       asciitilde ] };
    key <AB09> { [           ae,           AE,        question,       asciitilde ] };
    key <AB10> { [       period,        comma,           comma,       asciitilde ] };

    key <SPCE> { [        space,        space,           space,            space ] };

    include "altwin(alt_win)"
    include "level3(lalt_switch)"
    include "capslock(ctrl_modifier)"
};

// Russian layout for programmers
partial alphanumeric_keys
xkb_symbols "russian_for_programmers" {

    include "ru(common)"
    name[Group1]= "Russian (Russian for programmers)";

    key <TLDE> { [       Cyrillic_io,      Cyrillic_IO,      asciitilde,       asciitilde ] };
    key <AE01> { [                 1,           exclam,          exclam,       asciitilde ] };
    key <AE02> { [                 2,               at,              at,       asciitilde ] };
    key <AE03> { [                 3,       numbersign,      numbersign,       asciitilde ] };
    key <AE04> { [                 4,           dollar,          dollar,       asciitilde ] };
    key <AE05> { [                 5,          percent,         percent,       asciitilde ] };
    key <AE06> { [                 6,      asciicircum,     asciicircum,       asciitilde ] };
    key <AE07> { [                 7,        ampersand,       ampersand,       asciitilde ] };
    key <AE08> { [                 8,         asterisk,        asterisk,       asciitilde ] };
    key <AE09> { [                 9,        parenleft,       parenleft,       asciitilde ] };
    key <AE10> { [                 0,       parenright,      parenright,       asciitilde ] };
    key <AE11> { [             minus,       underscore,      underscore,       asciitilde ] };
    key <AE12> { [             equal,             plus,            plus,       asciitilde ] };

    key <AD01> { [   Cyrillic_shorti,   Cyrillic_SHORTI,         percent,       asciitilde ] };
    key <AD02> { [      Cyrillic_tse,      Cyrillic_TSE,     asciicircum,       asciitilde ] };
