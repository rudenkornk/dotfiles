    key <AD06> { [       Cyrillic_en,       Cyrillic_EN,             bar,       asciitilde ] };
    key <AD07> { [      Cyrillic_ghe,      Cyrillic_GHE,       ampersand,       asciitilde ] };
    key <AD08> { [      Cyrillic_sha,      Cyrillic_SHA,       braceleft,                8 ] };
    key <AD09> { [    Cyrillic_shcha,    Cyrillic_SHCHA,      braceright,                9 ] };
    key <AD10> { [       Cyriliic_ze,       Cyrillic_ZE,       semicolon,       asciitilde ] };
    key <AD11> { [       Cyrillic_ha,       Cyrillic_HA,      apostrophe,       asciitilde ] };
