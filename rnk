    key <AC03> { [       Cyrillic_ve,       Cyrillic_VE,            less,       asciitilde ] };
    key <AC04> { [        Cyrillic_a,        Cyrillic_A,           equal,       asciitilde ] };
    key <AC05> { [       Cyrillic_pe,       Cyrillic_PE,         greater,       asciitilde ] };
    key <AC06> { [       Cyrillic_er,       Cyrillic_ER,           slash,                0 ] };
    key <AC07> { [        Cyrillic_o,        Cyrillic_O,       parenleft,                1 ] };
    key <AC08> { [       Cyrillic_el,       Cyrillic_EL,      parenright,                2 ] };
    key <AC09> { [       Cyrillic_de,       Cyrillic_DE,      underscore,                3 ] };
    key <AC10> { [      Cyrillic_zhe,      Cyrillic_ZHE,           colon,                4 ] };
    key <AC11> { [        Cyrillic_e,        Cyrillic_E,        quotedbl,       asciitilde ] };

    key <AB01> { [       Cyrillic_ya,       Cyrillic_YA,      asciitilde,       asciitilde ] };
    key <AB02> { [      Cyrillic_che,      Cyrillic_CHE,      asciitilde,       asciitilde ] };
    key <AB03> { [       Cyrillic_es,       Cyrillic_ES,      asciitilde,       asciitilde ] };
    key <AB04> { [       Cyrillic_em,       Cyrillic_EM,          dollar,       asciitilde ] };
    key <AB05> { [        Cyrillic_i,        Cyrillic_I,       backslash,                5 ] };
    key <AB06> { [       Cyrillic_te,       Cyrillic_TE,     bracketleft,                6 ] };
    key <AB07> { [ Cyrillic_softsign, Cyrillic_SOFTSIGN,    bracketright,                7 ] };
    key <AB08> { [       Cyrillic_be,       Cyrillic_BE,      asciitilde,       asciitilde ] };
