{
  "providers": {
    "corp": {
      "name": "corp",
      "base_url": "$OPENAI_BASE_URL",
      "api_key": "$OPENAI_API_KEY",
      "type": "openai-compat",
      "models": [
        {
          "id": "Qwen3-Coder-30B-A3B-Instruct-FP8",
          "name": "Qwen3-Coder-30B-A3B-Instruct-FP8",
          "context_window": 256000,
          "default_max_tokens": 16384
        }
      ]
    }
  },
  "mcp": {
    "filesystem": {
      "type": "stdio",
      "command": "nix",
      "args": ["run", "github:utensils/mcp-nixos", "--"],
      "timeout": 120
    }
  },
  "lsp": {
    "nix": {
      "command": "nil"
    }
  }
}
