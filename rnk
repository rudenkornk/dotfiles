    key <AC04> { [        Cyrillic_a,        Cyrillic_A,           equal,            equal ] };
    key <AC05> { [       Cyrillic_pe,       Cyrillic_PE,         greater,          greater ] };
    key <AC06> { [       Cyrillic_er,       Cyrillic_ER,           slash,            slash ] };
    key <AC07> { [        Cyrillic_o,        Cyrillic_O,       parenleft,                1 ] };
    key <AC08> { [       Cyrillic_el,       Cyrillic_EL,      parenright,                2 ] };
    key <AC09> { [       Cyrillic_de,       Cyrillic_DE,      underscore,                3 ] };
    key <AC10> { [      Cyrillic_zhe,      Cyrillic_ZHE,           colon,                4 ] };
    key <AC11> { [        Cyrillic_e,        Cyrillic_E,       semicolon,        semicolon ] };

    key <AB01> { [       Cyrillic_ya,       Cyrillic_YA,      asciitilde,       asciitilde ] };
