    key <AB03> { [       Cyrillic_es,       Cyrillic_ES,      asciitilde,       asciitilde ] };
    key <AB04> { [       Cyrillic_em,       Cyrillic_EM,          dollar,           dollar ] };
    key <AB05> { [        Cyrillic_i,        Cyrillic_I,       backslash,        backslash ] };
    key <AB06> { [       Cyrillic_te,       Cyrillic_TE,     bracketleft,                5 ] };
    key <AB07> { [ Cyrillic_softsign, Cyrillic_SOFTSIGN,    bracketright,                6 ] };
    key <AB08> { [       Cyrillic_be,       Cyrillic_BE,        quotedbl,                7 ] };
