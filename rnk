    key <AD09> { [    Cyrillic_shcha,    Cyrillic_SHCHA,      braceright,                9 ] };
    key <AD10> { [       Cyriliic_ze,       Cyrillic_ZE,        question,         question ] };
    key <AD11> { [       Cyrillic_ha,       Cyrillic_HA,      asciitilde,       asciitilde ] };
